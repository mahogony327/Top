{
  "expo": {
    "name": "Top",
    "slug": "top",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0ea5e9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.top.rankings"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0ea5e9"
      },
      "package": "com.top.rankings"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "top",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
